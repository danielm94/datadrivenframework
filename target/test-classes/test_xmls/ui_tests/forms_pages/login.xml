<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Parallel Suite" verbose="1" parallel="classes">
    <test name="Chrome Suite">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="app.netlify.qaautomationpractice.ui_tests.forms.LoginUITest"/>
            <class name="app.netlify.qaautomationpractice.ui_tests.home_page.HomePageUITest"/>
        </classes>
    </test>
    <test name="Firefox Suite">
        <parameter name="browser" value="firefox"/>
        <classes>
            <class name="app.netlify.qaautomationpractice.ui_tests.forms.LoginUITest"/>
            <class name="app.netlify.qaautomationpractice.ui_tests.home_page.HomePageUITest"/>
        </classes>
    </test>
</suite>